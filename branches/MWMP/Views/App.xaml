<Application
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:local="clr-namespace:MWMP"
			 xmlns:s="clr-namespace:System;assembly=mscorlib" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:DataStore="clr-namespace:Expression.Blend.DataStore.DataStore" mc:Ignorable="d" x:Class="MWMP.App" 
             StartupUri="MainWindow.xaml">
    <Application.Resources>
		
        
    	<ResourceDictionary>
    		<ResourceDictionary.MergedDictionaries>
    			<ResourceDictionary Source="BaseStyle.xaml"/>
